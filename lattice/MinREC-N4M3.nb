                  RowBox[{
                   RowBox[{"f", "[", "i", "]"}], "\[Rule]", "x"}]}], "/.", 
                 RowBox[{
                  RowBox[{"f", "[", "a_", "]"}], "\[RuleDelayed]", 
                  RowBox[{"vals", "[", 
                   RowBox[{"[", 
                    RowBox[{"a", "+", "1"}], "]"}], "]"}]}]}], ",", "x"}], 
               "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "1", ",", "2"}], "]"}], "]"}]}], "]"}], 
