Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Given", " ", "the", " ", "first", " ", "values", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"f", "[", "0", "]"}], ",", "...", ",", 
       RowBox[{"f", "[", "m", "]"}]}], "}"}], " ", "of", " ", "a", " ", 
     "sequence", " ", 
     RowBox[{"f", "[", "n", "]"}], " ", "and", " ", "a", " ", "basis", " ", 
     "of", " ", "its", " ", "asymptotic", " ", "solutions"}], ",", " ", 
    RowBox[{"compute", " ", "the", " ", "limit", " ", 
     RowBox[{
      RowBox[{"Limit", "[", 
       RowBox[{
        RowBox[{"f", "[", "n", "]"}], ",", " ", 
        RowBox[{"n", "\[Rule]", "Infinity"}]}], "]"}], "."}]}]}], " ", "*)"}],
   "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "SeqLimit", "]"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"SeqLimit", "[", 
      RowBox[{"data_List", ",", "asym_", ",", "n_"}], "]"}], ":=", 
     "\[IndentingNewLine]", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"c", ",", 
         RowBox[{"d", "=", 
          RowBox[{"Length", "[", "asym", "]"}]}], ",", "pos", ",", "ansatz", 
         ",", "sol"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"pos", "=", 
         RowBox[{
          RowBox[{"Length", "[", "data", "]"}], "+", 
          RowBox[{"Range", "[", 
           RowBox[{
            RowBox[{"-", "d"}], ",", 
            RowBox[{"-", "1"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"ansatz", "=", 
         RowBox[{
          RowBox[{"Array", "[", 
           RowBox[{"c", ",", "d"}], "]"}], ".", "asym"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"sol", "=", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"ansatz", "/.", 
                  RowBox[{"n", "\[Rule]", "#"}]}], ")"}], "\[Equal]", 
                RowBox[{"data", "[", 
                 RowBox[{"[", 
                  RowBox[{"#", "+", "1"}], "]"}], "]"}]}], ")"}], "&"}], "/@",
              "pos"}], ",", 
            RowBox[{"Array", "[", 
             RowBox[{"c", ",", "d"}], "]"}]}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"c", "[", "d", "]"}], "/.", "sol"}], ",", "200"}], "]"}], 
         "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]}]}]], "Input",
