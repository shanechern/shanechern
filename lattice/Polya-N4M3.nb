Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"given", " ", "a", " ", "recurrence", " ", "rec", " ", "in", " ", 
     RowBox[{"f", "[", "n", "]"}]}], ",", " ", 
    RowBox[{"compute", " ", "the", " ", "values", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"f", "[", "0", "]"}], ",", 
       RowBox[{"f", "[", "1", "]"}], ",", "...", ",", 
       RowBox[{"f", "[", "bound", "]"}]}], "}"}], " ", "\[IndentingNewLine]", 
     "where", " ", "inits", " ", "are", " ", "the", " ", "initial", " ", 
     "values", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"f", "[", "0", "]"}], ",", "...", ",", 
       RowBox[{"f", "[", 
        RowBox[{"d", "-", "1"}], "]"}]}], "}"}], " ", "with", " ", "d", " ", 
     "being", " ", "the", " ", "order", " ", "of", " ", "the", " ", 
     "recurrence"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "UnrollRecurrence", "]"}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"UnrollRecurrence", "[", 
      RowBox[{"rec1_", ",", 
       RowBox[{"f_", "[", "n_", "]"}], ",", "inits_", ",", "bound_"}], "]"}], 
     ":=", "\[IndentingNewLine]", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"i", ",", "x", ",", 
         RowBox[{"vals", "=", "inits"}], ",", 
         RowBox[{"rec", "=", "rec1"}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Head", "[", "rec", "]"}], "=!=", "Equal"}], ",", 
          RowBox[{"rec", "=", 
           RowBox[{"(", 
            RowBox[{"rec", "\[Equal]", "0"}], ")"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"rec", "=", 
         RowBox[{"rec", "/.", 
          RowBox[{"n", "\[Rule]", 
           RowBox[{"n", "-", 
            RowBox[{"Max", "[", 
             RowBox[{"Cases", "[", 
              RowBox[{"rec", ",", 
               RowBox[{
                RowBox[{"f", "[", 
                 RowBox[{"n", "+", "a_."}], "]"}], "\[RuleDelayed]", "a"}], 
               ",", "Infinity"}], "]"}], "]"}]}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"AppendTo", "[", 
            RowBox[{"vals", ",", 
             RowBox[{
              RowBox[{"Solve", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"rec", "/.", 
                   RowBox[{"n", "\[Rule]", "i"}]}], "/.", 
                  RowBox[{
                   RowBox[{"f", "[", "i", "]"}], "\[Rule]", "x"}]}], "/.", 
                 RowBox[{
                  RowBox[{"f", "[", "a_", "]"}], "\[RuleDelayed]", 
                  RowBox[{"vals", "[", 
                   RowBox[{"[", 
                    RowBox[{"a", "+", "1"}], "]"}], "]"}]}]}], ",", "x"}], 
               "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "1", ",", "2"}], "]"}], "]"}]}], "]"}], 
           ";"}], "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"Length", "[", "inits", "]"}], ",", "bound"}], "}"}]}], 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "vals", "]"}], ";"}]}], "\[IndentingNewLine]", 
      "]"}]}], ";"}]}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{3.9249714646515107`*^9, 3.935127398817605*^9},
 CellLabel->"In[3]:=",ExpressionUUID->"1f0aaa3c-abe2-43a2-bda1-efb74a1b58f7"]
