dec60d10f805"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"NN", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MM", "=", "3"}], ";"}]}], "Input",
 CellChangeTimes->{{3.9228835055656395`*^9, 3.922883512352476*^9}, {
  3.923063479918706*^9, 3.923063481301264*^9}, {3.923068380094208*^9, 
  3.923068381439791*^9}, {3.923073834625342*^9, 3.9230738361068707`*^9}},
 CellLabel->"In[36]:=",ExpressionUUID->"880c358a-2e1d-49c5-b5ed-603cb5587135"],

Cell[TextData[{
 "Recall some basic definitions in the paper:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"2", " ", "\[Pi]"}], ")"}], "N"]], 
         SubsuperscriptBox["\[Integral]", 
          RowBox[{"-", "\[Pi]"}], "\[Pi]"]}], "..."}], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
        RowBox[{
         FractionBox["1", 
          RowBox[{"1", " ", "-", " ", 
           RowBox[{
            FractionBox["z", 
             RowBox[{"(", GridBox[{
                {"N"},
                {"M"}
               }], ")"}]], " ", 
            RowBox[{
             SubscriptBox["\[Sigma]", "M"], "(", 
             RowBox[{
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "1"]}], ",", " ", "...", ",", " ", 
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "N"]}]}], ")"}]}]}]], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["\[Theta]", "1"]}]}]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Theta]", "N"]}]}]}], TraditionalForm]],ExpressionUUID->
  "f34f53d6-5f26-4cb3-b3a2-219b8d130e5d"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", 
     RowBox[{
      SuperscriptBox["2", "M"], 
      RowBox[{"(", GridBox[{
         {"N"},
         {"M"}
        }], ")"}], "z"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "6c27b146-8cde-4a55-bc26-2d9575c66937"],
 "   and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"n", "\[GreaterEqual]", "0"}]], 
     RowBox[{
      RowBox[{
       SubscriptBox["r", 
        RowBox[{"M", ",", "N"}]], "(", "n", ")"}], 
      SuperscriptBox["z", "n"]}]}]}], TraditionalForm]],ExpressionUUID->
  "d8a76240-3538-49c8-a72a-287e60242c71"],
 "\n\nAlso, for ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "71960ebb-b288-498e-9cb6-8830634a5013"],
 " odd or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "=", "N"}], TraditionalForm]],ExpressionUUID->
  "0436df02-87e7-414b-912b-ab5bf4b81c91"],
 ", we always have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"r", "(", 
     RowBox[{
      RowBox[{"2", "n"}], "+", "1"}], ")"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"25a2f1c4-5a62-48eb-b37b-23a14e90b6c7"],
 ". Hence, define\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["r", "~"], 
      RowBox[{"M", ",", "N"}]], "(", "n", ")"}], ":=", 
    RowBox[{
     SubscriptBox["r", 
      RowBox[{"M", ",", "N"}]], "(", 
     RowBox[{"2", "n"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "d2824118-1d4b-4ca3-b723-a20446698ea4"],
 "  and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["R", "~"], 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"n", "\[GreaterEqual]", "0"}]], 
      RowBox[{
       RowBox[{
        SubscriptBox[
         OverscriptBox["r", "~"], 
         RowBox[{"M", ",", "N"}]], "(", "n", ")"}], 
       SuperscriptBox["z", "n"]}]}], "=", 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"n", "\[GreaterEqual]", "0"}]], 
      RowBox[{
       RowBox[{
        SubscriptBox["r", 
         RowBox[{"M", ",", "N"}]], "(", 
        RowBox[{"2", "n"}], ")"}], 
       SuperscriptBox["z", "n"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "bcaef655-e414-47e1-812e-138460b349be"],
 " \n\n",
 StyleBox["Our goal is to find the associated Polya number of the lattice in \
question.",
  FontSize->18,
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 0]]
}], "Text",
 CellChangeTimes->{{3.9248957780220613`*^9, 3.9248958467228394`*^9}, 
   3.924895968022427*^9, {3.9248960287387686`*^9, 3.9248966570001593`*^9}, {
   3.924896691742081*^9, 3.924896769357921*^9}, {3.92489682264349*^9, 
   3.924897149138559*^9}, {3.9351273545742755`*^9, 
   3.9351273815523696`*^9}},ExpressionUUID->"135385b5-97b2-466a-ad46-\
4d8c40483465"],

