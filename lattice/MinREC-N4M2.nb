Cell[TextData[{
 "Recall some basic definitions in the paper:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"2", " ", "\[Pi]"}], ")"}], "N"]], 
         SubsuperscriptBox["\[Integral]", 
          RowBox[{"-", "\[Pi]"}], "\[Pi]"]}], "..."}], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
        RowBox[{
         FractionBox["1", 
          RowBox[{"1", " ", "-", " ", 
           RowBox[{
            FractionBox["z", 
             RowBox[{"(", GridBox[{
                {"N"},
                {"M"}
               }], ")"}]], " ", 
            RowBox[{
             SubscriptBox["\[Sigma]", "M"], "(", 
             RowBox[{
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "1"]}], ",", " ", "...", ",", " ", 
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "N"]}]}], ")"}]}]}]], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["\[Theta]", "1"]}]}]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Theta]", "N"]}]}]}], TraditionalForm]],ExpressionUUID->
  "c1849520-aadb-42b5-8b25-b4f5c6490852"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", 
     RowBox[{
      SuperscriptBox["2", "M"], 
      RowBox[{"(", GridBox[{
         {"N"},
         {"M"}
        }], ")"}], "z"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "f9dec07e-d1c6-4dea-9a39-86d8d8afbf32"],
 "   and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"n", "\[GreaterEqual]", "0"}]], 
     RowBox[{
      RowBox[{
       SubscriptBox["r", 
        RowBox[{"M", ",", "N"}]], "(", "n", ")"}], 
      SuperscriptBox["z", "n"]}]}]}], TraditionalForm]],ExpressionUUID->
  "1b240952-bbcf-47ef-9643-20fc66343a86"],
