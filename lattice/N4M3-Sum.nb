Cell[TextData[{
 "Recall some basic definitions in the paper:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"2", " ", "\[Pi]"}], ")"}], "N"]], 
         SubsuperscriptBox["\[Integral]", 
          RowBox[{"-", "\[Pi]"}], "\[Pi]"]}], "..."}], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
        RowBox[{
         FractionBox["1", 
          RowBox[{"1", " ", "-", " ", 
           RowBox[{
            FractionBox["z", 
             RowBox[{"(", GridBox[{
                {"N"},
                {"M"}
               }], ")"}]], " ", 
            RowBox[{
             SubscriptBox["\[Sigma]", "M"], "(", 
             RowBox[{
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "1"]}], ",", " ", "...", ",", " ", 
              RowBox[{"cos", " ", 
               SubscriptBox["\[Theta]", "N"]}]}], ")"}]}]}]], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["\[Theta]", "1"]}]}]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Theta]", "N"]}]}]}], TraditionalForm]],ExpressionUUID->
  "0d829231-4284-4be7-8626-644a82a52c24"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", ",", "N"}]], "(", 
     RowBox[{
      SuperscriptBox["2", "M"], 
      RowBox[{"(", GridBox[{
         {"N"},
         {"M"}
        }], ")"}], "z"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a349da44-5eca-4734-86a4-874d4a396269"],
 "   and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"n", "\[GreaterEqual]", "0"}]], 
     RowBox[{
      RowBox[{
       SubscriptBox["r", 
        RowBox[{"M", ",", "N"}]], "(", "n", ")"}], 
      SuperscriptBox["z", "n"]}]}]}], TraditionalForm]],ExpressionUUID->
  "067b2f1c-3aad-46ac-a255-e53ef441b6ec"],
 "\n\nAlso, for ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "3bf5b5a8-435e-46e5-a73d-4a7714518718"],
 " odd or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "=", "N"}], TraditionalForm]],ExpressionUUID->
  "3a36755c-2653-4f25-bd0a-f928ea67398e"],
 ", we always have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"r", "(", 
     RowBox[{
      RowBox[{"2", "n"}], "+", "1"}], ")"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"4f8c2340-0170-4302-a721-dccce6fa5496"],
 ". Hence, define\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["r", "~"], 
      RowBox[{"M", ",", "N"}]], "(", "n", ")"}], ":=", 
    RowBox[{
     SubscriptBox["r", 
      RowBox[{"M", ",", "N"}]], "(", 
     RowBox[{"2", "n"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "bc61bfc8-fc62-40dd-8b95-32bda04b0eac"],
 "  and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["R", "~"], 
      RowBox[{"M", ",", "N"}]], "(", "z", ")"}], ":=", 
    RowBox[{
