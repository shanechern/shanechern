       RowBox[{"n", "\[GreaterEqual]", "0"}]], 
      RowBox[{
       RowBox[{
        SubscriptBox[
         OverscriptBox["r", "~"], 
         RowBox[{"M", ",", "N"}]], "(", "n", ")"}], 
       SuperscriptBox["z", "n"]}]}], "=", 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"n", "\[GreaterEqual]", "0"}]], 
      RowBox[{
       RowBox[{
        SubscriptBox["r", 
         RowBox[{"M", ",", "N"}]], "(", 
        RowBox[{"2", "n"}], ")"}], 
       SuperscriptBox["z", "n"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a5629c0c-0602-4eac-915f-afc7f6eb37c7"],
